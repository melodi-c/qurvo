{
  "name": "@qurvo/ai-types",
  "version": "1.0.0",
  "description": "Zod output schemas and TypeScript types for Qurvo AI tool results",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/cjs/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.esm.json",
    "dev": "tsc -p tsconfig.json --watch",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@qurvo/tsconfig": "workspace:*",
    "typescript": "^5.7.0"
  }
}
